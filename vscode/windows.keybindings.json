// 키 바인딩을 이 파일에 넣어서 기본값 재정의
[
  // Vim 키 충돌 해결 (vim.handleKeys 으로 해결안되는 것을 명시)
  {
    "key": "shift+alt+up",
    "command": "-extension.vim_cmd+alt+up"
  },
  {
    "key": "shift+alt+down",
    "command": "-extension.vim_cmd+alt+down"
  },
  // NeoVim 키 충돌 해결
  {
    "key": "ctrl+z",
    "command": "-vscode-neovim.send"
  },
  {
    "key": "ctrl+c",
    "command": "-vscode-neovim.escape"
  },
  {
    "key": "ctrl+v",
    "command": "-vscode-neovim.send"
  },
  {
    "key": "ctrl+f",
    "command": "-vscode-neovim.send"
  }
  // EOF
]
